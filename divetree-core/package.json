{
  "name": "divetree-core",
  "version": "0.1.0",
  "description": "Displays a tree which you can dive into interactively.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://git.dolansoft.org/philippe/divetree"
  },
  "keywords": ["tree", "dive", "explore", "display"],
  "author": "Philippe \"tehwalris\" Voinov",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^22.1.2",
    "@types/node": "^9.4.5",
    "@types/ramda": "^0.25.17",
    "jest": "^22.2.2",
    "jest-runner": "^22.3.0",
    "ts-jest": "^22.0.4",
    "tsc": "^1.20150623.0",
    "typescript": "^2.7.1"
  },
  "dependencies": {
    "d3-flextree": "^2.1.1",
    "kiwi.js": "^1.1.0",
    "ramda": "^0.25.0",
    "tree-crawl": "^1.0.5"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "rootDir": "src",
    "testRegex": "(/__tests__/.*)\\.(tsx?)$",
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
    "collectCoverage": true,
    "mapCoverage": true,
    "coverageDirectory": "../coverage"
  }
}
