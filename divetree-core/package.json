{
  "name": "divetree-core",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "description": "Displays a tree which you can dive into interactively.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://git.dolansoft.org/philippe/divetree"
  },
  "keywords": [
    "tree",
    "dive",
    "explore",
    "display"
  ],
  "author": "Philippe \"tehwalris\" Voinov",
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.171",
    "@types/node": "^16.3.2",
    "@types/ramda": "^0.27.44",
    "jest": "^27.0.6",
    "jest-runner": "^27.0.6",
    "lodash": "^4.17.21",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "d3-flextree": "^2.1.1",
    "ramda": "^0.27.1",
    "tree-crawl": "^1.1.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "rootDir": "src",
    "testRegex": "(/__tests__/.*)\\.(tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverage": true,
    "coverageDirectory": "../coverage"
  }
}
