{
  "name": "divetree-core",
  "version": "0.1.0",
  "description": "Displays a tree which you can dive into interactively.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://git.dolansoft.org/philippe/divetree"
  },
  "keywords": [
    "tree",
    "dive",
    "explore",
    "display"
  ],
  "author": "Philippe \"tehwalris\" Voinov",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^24.0.25",
    "@types/node": "^13.1.1",
    "@types/ramda": "^0.26.39",
    "jest": "^24.9.0",
    "jest-runner": "^24.9.0",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "d3-flextree": "^2.1.1",
    "kiwi.js": "^1.1.0",
    "ramda": "^0.26.1",
    "tree-crawl": "^1.0.5"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "rootDir": "src",
    "testRegex": "(/__tests__/.*)\\.(tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverage": true,
    "mapCoverage": true,
    "coverageDirectory": "../coverage"
  }
}
